  object SQLTotalContagem: TRxQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'SELECT'
      '  sum(INVENTARIOESTOQUE.INVICONTAGEM1) as Total1,'
      '  sum(INVENTARIOESTOQUE.INVICONTAGEM2) as Total2,'
      '  sum(INVENTARIOESTOQUE.INVICONTAGEM3) as Total3,'
      
        '  sum(INVENTARIOESTOQUE.INVICONTAGEM1*PRODUTO.PRODN3VLRCUSTO) as' +
        ' TotalCusto1'
      'FROM'
      
        '  INVENTARIOESTOQUE INVENTARIOESTOQUE inner join Produto on INVE' +
        'NTARIOESTOQUE.PRODICOD = Produto.PRODICOD'
      'WHERE %MFILTRO'
      '')
    Macros = <
      item
        DataType = ftString
        Name = 'MFILTRO'
