  object SQLProduto: TRxQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'SELECT'
      '  PRO.PRODICOD,'
      '  PRO.PRODA60CODBAR,'
      '  PRO.PRODA15CODANT,'
      '  PRO.PRODA60DESCR,'
      '  PRO.GRUPICOD,'
      '  PRO.SUBGICOD,'
      '  PRO.VARIICOD,'
      '  PRO.MARCICOD,'
      '  GRU.GRUPA60DESCR,'
      '  SUB.SUBGA60DESCR,'
      '  VAR.VARIA60DESCR,'
      '  MAR.MARCA60DESCR'
      'FROM'
      '  PRODUTO PRO'
      '  LEFT OUTER JOIN GRUPO GRU ON PRO.GRUPICOD = GRU.GRUPICOD'
      
        '  LEFT OUTER JOIN SUBGRUPO SUB ON PRO.GRUPICOD = SUB.GRUPICOD AN' +
        'D'
      '    PRO.SUBGICOD = SUB.SUBGICOD'
      
        '  LEFT OUTER JOIN VARIACAO VAR ON PRO.GRUPICOD = VAR.GRUPICOD AN' +
        'D'
      '    PRO.SUBGICOD = VAR.SUBGICOD AND'
      '    PRO.VARIICOD = VAR.VARIICOD'
      '  LEFT OUTER JOIN MARCA MAR ON PRO.MARCICOD = MAR.MARCICOD'
      'WHERE'
      '  %MPRODICOD AND'
      '  %MPRODA15CODANT AND'
      '  %MPRODA60CODBAR'
      '  ')
