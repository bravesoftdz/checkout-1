      '  CLIENTE.CLIEDNASC as DATANASCIMENTO,'
      '  CLIENTE.CLIEA11CPF as CPF,'
      '  CLIENTE.CLIEA10RG as RG,'
      '  CLIENTE.CLIEA60CONJUGE,'
      '  CARTAAVISOSPC.CTRCN2VLR as VALOR,'
      '  CARTAAVISOSPC.AVSPDVENC as VENCTO,'
      '  CUPOM.CUPODEMIS as EMISSAO,'
      '  CLIENTE.CLIECESTADOCIVIL,'
      '  CLIENTE.CLIETOBS,'
      '  CARTAAVISOSPC.AVSPDPROCESS,'
      '  CONTASRECEBER.CTRCA5TIPOPADRAO,'
