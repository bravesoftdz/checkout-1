  object SQLResumo: TRxQuery
    DatabaseName = 'UnitGestao_Temp'
    SQL.Strings = (
      'select '
      'INSUMOSEMIACABADOTIPO as Tipo, '
      'INSUMOSEMIACABADO as Cod, '
