  object SQLCountTOTAL: TRxQuery
    AfterOpen = SQLCountTOTALAfterOpen
    DatabaseName = 'DB'
    SQL.Strings = (
      'select '
      'PRODUTO.PRODICOD,'
      'PRODUTO.PRODA60DESCR,'
      'PRODUCAOFASES.PRFAA30DESCR,'
      'Sum(PRODUCAOFASES.PRFAN2TEMPOTOTAL) as TempoTotal,'
      'Sum(PRODUCAOFASES.PRFAN2CAPACPROD) as Capacidade,'
      
        'Sum(PRODUCAOFASES.PRFAN2TEMPOTOTAL)  /  Sum(PRODUCAOFASES.PRFAN2' +
        'CAPACPROD) as Media'
      ' from '
      'PRODUTO '
      
        'left outer join PRODUTOPRODUCAO on PRODUTOPRODUCAO.PRODICOD = PR' +
        'ODUTO.PRODICOD'
      
        'left outer join PRODUCAOPRODUTOFASE on PRODUCAOPRODUTOFASE.PRODI' +
        'COD = PRODUTOPRODUCAO.PRODICOD'
      
        'left outer join PRODUCAOFASES on PRODUCAOFASES.PRFAA13ID = PRODU' +
        'CAOPRODUTOFASE.PRFAA13ID'
      'where '
      'PRODUTO.PRODCTIPO = '#39'P'#39' '
      'and '
      '(%MFiltro)'
      'group by'
      'PRODUTO.PRODICOD,'
      'PRODUTO.PRODA60DESCR,'
      'PRODUCAOFASES.PRFAA30DESCR'
      ''
      'order by PRODUTO.PRODA60DESCR')
    Macros = <
      item
        DataType = ftString
        Name = 'MFiltro'
        ParamType = ptInput
        Value = '0=0'
      end>
