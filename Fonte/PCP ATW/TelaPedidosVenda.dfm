  object SQLFichaTecResumida: TRxQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select distinct'
      'PRODUTORAIZ.PRODICOD           as ProdutoRaiz,'
      'PRODUTORAIZ.PRODA60DESCR       as ProdutoRaizNome,'
      'PRODCOMPPAI.PRODN3QTDE         as ProdutoQtde,'
      'PRODCOMPPAI.PRODICODFILHO      as InsumoSemiAcabado,'
      'PRODFILHONOME.PRODA60DESCR     as InsumoSemiAcabadoNome,'
      'PRODFILHONOME.PRODCTIPO        as InsumoSemiAcabadoTipo,'
      'PRODFILHONOME.PRODN3CAPACEMBAL as InsumoSemiAcabadoQtde,'
      'PEDIDOVENDAITEM.PVITN3QUANT,'
      'PRODCOMPPAI.PRODN3QTDE,'
      'PRODFILHONOME.PRODN3CAPACEMBAL,'
      ''
      
        '(PEDIDOVENDAITEM.PVITN3QUANT * PRODFILHONOME.PRODN3CAPACEMBAL)  ' +
        '* PRODCOMPPAI.PRODN3QTDE as InsumoSemiAcabadoTotal,'
      'PRODCOMPFILHO.PRODICODFILHO    as Insumo,'
      'PRODFILHONETO.PRODA60DESCR     as InsumoNome,'
      'PRODFILHONETO.PRODCTIPO        as InsumoTipo,'
      'PRODFILHONETO.PRODN3CAPACEMBAL as InsumoQtde,'
      'PEDIDOVENDAITEM.PVITN3QUANT,'
      
        '((PEDIDOVENDAITEM.PVITN3QUANT * PRODCOMPPAI.PRODN3QTDE) * PRODFI' +
        'LHONOME.PRODN3CAPACEMBAL) * PRODCOMPFILHO.PRODN3QTDE as InsumoTo' +
        'tal'
      'from PEDIDOVENDAITEM '
      
        'left outer join PRODUTO PRODUTORAIZ on PRODUTORAIZ.PRODICOD = PE' +
        'DIDOVENDAITEM.PRODICOD'
      
        'left outer join PRODUTOCOMPOSICAO PRODCOMPPAI on PRODCOMPPAI.PRO' +
        'DICOD = PRODUTORAIZ.PRODICOD'
      
        'left outer join PRODUTO PRODFILHONOME on PRODFILHONOME.PRODICOD ' +
        '= PRODCOMPPAI.PRODICODFILHO'
      
        'left outer join PRODUTOCOMPOSICAO PRODCOMPFILHO on PRODCOMPFILHO' +
        '.PRODICOD = PRODCOMPPAI.PRODICODFILHO'
      
        'left outer join PRODUTO PRODFILHONETO on PRODFILHONETO.PRODICOD ' +
        '= PRODCOMPFILHO.PRODICODFILHO'
      'where'
      '(%MFiltro)')
    Macros = <
      item
        DataType = ftString
        Name = 'MFiltro'
        ParamType = ptInput
        Value = '0=0'
      end>
