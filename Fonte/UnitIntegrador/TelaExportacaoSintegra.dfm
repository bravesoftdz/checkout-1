  object SQLNotaFiscal: TRxQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'SELECT * FROM NOTAFISCAL'
      'WHERE'
      '  (%MEmpresa)'
      'and'
      '  (NOFICSTATUS = "E" or NOFICSTATUS = "C")'
      'and'
      '  (%MSerie)'
      'and '
      '  SERIA5COD <>  "D1"'
      'and'
      '  (%CampoData Between %DataInicial and %DataFinal)'
      'ORDER BY NOFIINUMERO'
      '')
