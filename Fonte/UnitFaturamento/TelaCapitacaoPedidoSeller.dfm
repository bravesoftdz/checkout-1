  object SQLTemplate: TRxQuery
    DatabaseName = 'DB'
    RequestLive = True
    SQL.Strings = (
      'Select '
      '*'
      'From'
      
        '(PREVENDA PREVENDA left outer join CLIENTE CLIENTE ON PREVENDA.C' +
        'LIEA13ID = CLIENTE.CLIEA13ID'
      
        'left outer join VENDEDOR VENDEDOR on PREVENDA.VENDICOD = VENDEDO' +
        'R.VENDICOD'
      
        'left outer join TERMINAL TERMINAL on PREVENDA.TERMICOD = TERMINA' +
        'L.TERMICOD)'
      'Where'
      'PRVDCIMPORT = '#39'N'#39
      'and'
      '(%MData)')
    Macros = <
      item
        DataType = ftString
        Name = 'MData'
        ParamType = ptInput
        Value = '0=0'
      end>
    Left = 447
    Top = 3
  end
